{
	"compilerOptions": {
		"esModuleInterop": true,
		"skipLibCheck": true,
		"target": "es2022",
		"allowJs": true,
		"resolveJsonModule": true,
		"moduleDetection": "force",
		"isolatedModules": true,
		"strict": true,
		"noUncheckedIndexedAccess": false,
		"noImplicitOverride": true,
		"module": "NodeNext",
		"sourceMap": true,
		"lib": ["es2022"],
		"outDir": "dist"
	}
}
